SELECT sale.pn, GROUPING(sale.pn) + 1 as g1 FROM product, sale WHERE product.pn=sale.pn GROUP BY ROLLUP((sale.pn,product.pname,sale.pn)) ORDER BY 1,2;
 pn  | g1 
-----+----
 100 |  1
 200 |  1
 300 |  1
 400 |  1
 500 |  1
 600 |  1
 700 |  1
 800 |  1
     |  2
